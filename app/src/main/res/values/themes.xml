<resources>
    <!-- Base application theme. -->

    <style name="Theme.PRSpeedyPickup" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/Rashadcolor</item>
        <item name="colorPrimaryDark">@color/Black</item>
        <item name="colorAccent">@color/darkgray</item>


    </style>

    <style name="AppThemeNoActionbar" parent="Theme.PRSpeedyPickup">
        <item name="windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
    </style>

    <style name="Theme.PRSpeedyPickup.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.PRSpeedyPickup.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.PRSpeedyPickup.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>